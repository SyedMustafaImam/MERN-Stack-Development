<!doctype html>
<html lang="en">

<%- include('./partials/head.ejs') %>


    <body>
        <header>
            <%- include( './partials/nav.ejs' ) %>

        </header>
        <main>
            <div class="row">
                <!-- <div class="col 2s">
                    <%- include('./partials/dashboard.ejs') %>


                </div> -->
                <div class="container center-align" style="align-items: center;  align-content: center">

                    <center>
                        <div class="col 12s" style="align-items: center; align-content: center; padding-left: 25%;">

                            <h2 class="center-align">Profile Information</h2>
                            <hr>
                            <div class="container" style="border: solid 1px; border-radius: 25px; width: 100%;">
                                <table class="center-align">


                                    <tbody>
                                        <tr>
                                            <td>
                                                <h5>Name:</h5>
                                            </td>
                                            <td>
                                                <h5><%= session.name %></h5>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h5>Gender:</h5>
                                            </td>
                                            <td>
                                                <h5><%= session.Gender %></h5>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h5>Address:</h5>
                                            </td>
                                            <td>
                                                <h5><%= session.address %></h5>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h5>City:</h5>
                                            </td>
                                            <td>
                                                <h5><%= session.city %></h5>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h5>Country:</h5>
                                            </td>
                                            <td>
                                            <h5><%= session.country %></h5>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>

                        </div>
                    </center>

                </div>


            </div>

        </main>
        <footer>

            <%- include( './partials/footer.ejs' ) %>

        </footer>


    </body>

</html>